{
  "expo": {
    "name": "HealthyFoodies",
    "slug": "HealthyFoodies",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#FAFFF4"
    },
    "packagerOpts": {
      "sourceExts": [
        "cjs"
      ]
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo.png",
        "backgroundColor": "#FAFFF4"
      },
      "package": "com.HealthyFoodies.HealthyFoodies"
    }
  }
}
